{
  "name": "harmonia-shortcuts",
  "displayName": "Harmonia Shortcuts",
  "description": "Governance over keyboard shortcuts. Consciously decide which extension shortcuts should be active.",
  "version": "1.0.0",
  "publisher": "AgusRdz",
  "license": "MIT",
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgusRdz/harmonia-shortcuts"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Keymaps",
    "Other"
  ],
  "keywords": [
    "keybindings",
    "shortcuts",
    "keyboard",
    "governance",
    "audit"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "harmonia-shortcuts.openAudit",
        "title": "%command.openAudit%",
        "category": "Harmonia Shortcuts",
        "icon": "$(keyboard)"
      }
    ],
    "configuration": {
      "title": "Harmonia Shortcuts",
      "properties": {
        "harmonia-shortcuts.notifyOnNewShortcuts": {
          "type": "boolean",
          "default": true,
          "description": "%config.notifyOnNewShortcuts%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npx tsc -p ./",
    "watch": "npx tsc -watch -p ./",
    "lint": "npx eslint src --ext ts",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/codicons": "^0.0.44",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  }
}